
def minCoins(COINS,VAL):
    
    if VAL == 0:
        return 0 

    for i in COINS:
        
        #print(KAMTARIN)
        if i <= VAL:
            
            #print(f"{VAL} is VAL")
            KAMTARIN = VAL - i
            #LISTOFCOINS = []
            #LISTOFCOINS.append(i)
            #print (f"{KAMTARIN} is val - i")
            #print (LISTOFCOINS)
            print (i)
            return minCoins(COINS,KAMTARIN)
    
              
    
        
Adad= 11
Seke = [7,6,5,2,2]
minCoins(Seke,Adad)
